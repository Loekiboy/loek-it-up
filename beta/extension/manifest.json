{
  "manifest_version": 3,
  "name": "Loek it Up Importer",
  "version": "1.0.0",
  "description": "Importeer StudyGo woordenlijsten direct in Loek it Up",
  "icons": {
    "16": "icons/icon16.svg",
    "48": "icons/icon48.svg",
    "128": "icons/icon128.svg"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ]
}
